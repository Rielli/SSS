package com.sss.MasterController;

public class Enemies {
	private static String[] enemies;
	public Enemies() {
		enemies = new String[18];
		enemies[0] = "Cruiser";
		enemies[1] = "Stealth Ship";
		enemies[] = "";
		enemies[] = "";
		enemies[] = "";
		enemies[] = "";
		enemies[] = "";
		enemies[] = "";
		enemies[] = "";
		enemies[] = "";
		enemies[] = "";
		enemies[] = "";
		enemies[] = "";
		enemies[] = "";
		enemies[] = "";
		enemies[] = "";
		enemies[] = "";
		enemies[] = "";
	}
	public static String getEnemy(int i) {
		return enemies[i];
	}
}
