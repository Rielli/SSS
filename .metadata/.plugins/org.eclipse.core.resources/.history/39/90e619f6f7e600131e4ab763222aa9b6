package com.sss.MasterController;

public class Events {
	private static String[] event;
	public Events() {
		event = new String[11];
		event[0] = "Enemy Health Gain";
		event[1] = "Solar Storm";
		event[2] = "Meteor Shower";
		event[3] = "Black Hole";
		event[4] = "Worm Hole";
		event[5] = "";
		event[6] = "";
		event[7] = "";
		event[8] = "";
		event[9] = "";
		event[10] = "";
	}
	public String getEvent(int i) {
		return event[i];
	}
}